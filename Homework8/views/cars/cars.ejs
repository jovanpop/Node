<%- include ../partials/header%>
      <table class="table table-dark table-hover mt-5">
        <thead>
          <tr>
              <th>Brand</th>
              <th>Model</th>
              <th class="text-end">Year</th>
              <th class="text-end">Edit</th>
          </tr>
      </thead>
      <tbody>
              <% cars.map ((car)=>{%>
              <tr>
                  <td><%= car.brand %></td>
                  <td><%= car.model %></td>
                  <td class="text-end"><%= car.year %></td>
                  <td class="text-end"><a href="/cars/<%= car._id %>" class="btn btn-warning">Update</a></td>
              </tr>
              <% }) %>
      </tbody>
      </table>
      <div class="d-grid gap-2 mt-2">
        <a href="/cars/create" class="btn btn-primary bg-warning text-black">Add new car</a>
      </div>
    </div>
    <%- include ../partials/footer %>